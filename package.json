    "babel-preset-stage-0": "^6.22.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-rimraf": "^0.2.1",
    "npm-run-all": "^4.0.2",
    "react-server": "^0.6.0"
  },
